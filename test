var async = require('async');


var array = ['SarahBrownUK', 'denverfoodguy', 'BrianBrownNet', 'RichardPBacon', 'eddieizzard', 'stephenfry', 'umairh', 'rustyrockets', 'tinchystryder', 'HilaryAlexander', 'Zee', 'jemimakiss', 'RichardDawkins' ];
var calls = 0;
async.forever(function(next){
	data = array.shift();
	doSomething(data, function(){
		array.push(data);
		setImmediate(next);
	});


}, function(err){
	console.log(err);
});

function doSomething(user, callback){
	setTimeout(function(){
		console.log(user);
		callback();
	}, 5000);
}